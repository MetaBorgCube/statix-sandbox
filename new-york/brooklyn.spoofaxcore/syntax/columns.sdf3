module columns

imports
  brooklyn-lex
  types

context-free sorts ColumnRef ColumnDef ColumnConstraint

context-free syntax

  ColumnRef.ColumnRef = <<ID>>
  ColumnRef.QColumnRef = <<ID>.<ID>>
  
  ColumnDef.ColumnDef = <<ID> <DataType>>
  
  ColumnConstraint.PrimaryKey 	= <PRIMARY KEY> {case-insensitive}
  ColumnConstraint.NotNull		= <NOT NULL> {case-insensitive}
