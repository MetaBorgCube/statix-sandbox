module function

imports base

context-free syntax

  Type.FunT = [[Type] -> [Type]] {right}
  TYPE.FUN  = [[TYPE] -> [TYPE]] {right}
  
context-free syntax // function literals

  Exp.App = <<Exp> <Exp>> {left}

  Exp.Fun = <
    fun(<ID> : <Type>) 
      <Exp> 
    
  >
  Exp.FunRT = <
    fun(<ID> : <Type>) : <Type> 
      <Exp> 
    
  >
  Exp.FunUT = <
    fun(<ID>)  
      <Exp> 
    
  >  
  
template options

  ID = keyword {reject}
  keyword -/- [a-zA-Z0-9]